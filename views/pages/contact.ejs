<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/header'); -%>
  <body>
    <%- include('../components/navbar'); -%>

    <div id="contact" class="form-1 bg-gray">
      <div class="container">
        <div class="row">
          <div class="col-lg-6">
            <div class="text-container">
              <h2>Contact details</h2>
              <p class="sub-heading">
                It's easy to get in touch with me and inquire anything about
                your project , devlopment and designing
                <a class="orange no-line" href="#your-link">services</a>
              </p>

              <ul class="list-unstyled li-space-lg">
                <li>New Delhi , India</li>
                <li>
                  <a class="orange no-line" href="mailto:office@domain.com"
                    >anagh9931@gmail.com</a
                  >
                </li>
              </ul>
              <!-- end of contact details -->

              <div class="social-icons">
                <span class="fa-stack">
                  <a href="https://github.com/anagh9">
                    <i class="fas fa-circle fa-stack-2x"></i>
                    <i class="fab fa-github fa-stack-1x"></i>
                  </a>
                </span>
                <span class="fa-stack">
                  <a href="https://twitter.com/anagh9931">
                    <i class="fas fa-circle fa-stack-2x"></i>
                    <i class="fab fa-twitter fa-stack-1x"></i>
                  </a>
                </span>
                <span class="fa-stack">
                  <a href="https://www.instagram.com/roxtaranagh/">
                    <i class="fas fa-circle fa-stack-2x"></i>
                    <i class="fab fa-instagram fa-stack-1x"></i>
                  </a>
                </span>
              </div>
            </div>
          </div>
          <div class="col-lg-6">
            <!-- Contact Form -->
            <form
              name="contact-form"
              id="contactForm"
              action="/contact"
              method="POST"
            >
              <div class="form-group">
                <input
                  class="form-control-input"
                  name="name"
                  id="cname"
                  required
                />
                <label type="text" for="name" class="label-control">Name</label>
              </div>

              <div class="form-group">
                <input
                  type="email"
                  class="form-control-input"
                  id="cemail"
                  name="_replyto"
                  required
                />
                <label class="label-control" for="email">Email</label>
              </div>

              <div class="form-group">
                <textarea
                  class="form-control-textarea"
                  id="cmessage"
                  name="message"
                  required
                ></textarea>
                <label class="label-control" for="cmessage"
                  >Project details</label
                >
              </div>

              <div class="form-group">
                <button type="submit" class="form-control-submit-button">
                  Submit
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
<%- include('../partials/foot'); -%>
